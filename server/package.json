    "@nestjs/core": "^8.4.6",
    "@nestjs/microservices": "^8.4.6",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/websockets": "^8.4.6",
    "amqplib": "^0.10.4",
    "axios": "^1.4.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
